{
  "compilerOptions": {
    "target": "ES2022",
    "module": "ESNext",
    "strict": true,
    "jsx": "react-jsx",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "allowImportingTsExtensions": true, 
    "moduleResolution": "node",
    "lib": ["dom", "ES2023"],
    "noEmit": true
  },
  "include": ["src"]
}
